def __init__(self, nombre, apellido, anno=None, mes=None, dia=None):\n","    # objeto.atributo = valor asignado\n","    self.nombre = nombre\n","    self.apellido = apellido\n","    self.yyyy = anno\n","    self.mm = mes\n","    self.dd = dia\n","\n","  def nombre_completo(self):\n","    return f\"{self.nombre} {self.apellido}\"\n","\n","  def obtener_fecha_nacimiento_EEUU(self):\n","      # Diccionario para convertir el número del mes a su nombre en inglés\n","      meses_en_ingles = {\n","          1: \"Enero\",\n","          2: \"Febrero\",\n","          3: \"Marzo\",\n","          4: \"Abril\",\n","          5: \"Mayo\",\n","          6: \"Junio\",\n","          7: \"Julio\",\n","          8: \"Agosto\",\n","          9: \"Septiembre\",\n","          10: \"Octubre\",\n","          11: \"Noviembre\",\n","          12: \"Diciembre\"\n","      }\n","\n","      # Obtener el nombre del mes\n","      nombre_mes = meses_en_ingles[self.mm]\n","\n","      # Formatear la fecha de nacimiento\n","      fecha_nacimiento_EEUU = f\"{nombre_mes} {self.dd}, {self.yyyy}\"\n","\n","      return fecha_nacimiento_EEUU\n","\n","  def obtener_fecha_nacimiento(self):\n","      if(self.yyyy is None or self.mm is None or self.dd is None):\n","        return\n","\n","      # Diccionario para convertir el número del mes a su nombre en inglés\n","      meses_en_ingles = {\n","          1: \"Enero\",\n","          2: \"Febrero\",\n","          3: \"Marzo\",\n","          4: \"Abril\",\n","          5: \"Mayo\",\n","          6: \"Junio\",\n","          7: \"Julio\",\n","          8: \"Agosto\",\n","          9: \"Septiembre\",\n","          10: \"Octubre\",\n","          11: \"Noviembre\",\n","          12: \"Diciembre\",\n","      }\n","\n","      # Obtener el nombre del mes\n","      nombre_mes = meses_en_ingles[self.mm]\n","\n","      # Formatear la fecha de nacimiento\n","      fecha_nacimiento  = f\"{self.yyyy}-{self.mm}-{self.dd}\"\n","\n","      return fecha_nacimiento"],"metadata":{"